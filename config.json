{
    "!encryption-password": "password",
    "audio": "pipewire",
    "bootloader": "systemd-bootctl",
    "custom-commands": [
        "cd /root/; git clone -b add-base https://github.com/MiguelNdeCarvalho/ansible-pc.git",
        "cd /root/ansible-pc/; ansible-playbook -i localhost main.yml; rm -rf /root/ansible-pc/"
    ],
    "filesystem": "ext4",
    "harddrive": [
        "/dev/nvme0n1"
    ],
    "hostname": "mike-teste",
    "kernels": [
        "linux"
    ],
    "keyboard-language": "pt-latin1",
    "mirror-region": {
        "Portugal": {
            "https://glua.ua.pt/pub/archlinux/$repo/os/$arch": true,
            "https://ftp.rnl.tecnico.ulisboa.pt/pub/archlinux/$repo/os/$arch": true
        },
        "Spain": {
            "https://mirror.cloroformo.org/archlinux/$repo/os/$arch": true
        },
        "France": {
            "https://mirror.cyberbits.eu/archlinux/$repo/os/$arch": true,
            "https://mirrors.eric.ovh/arch/$repo/os/$arch": true,
            "https://arch-mirror.cloud.louifox.house/$repo/os/$arch": true,
            "https://archlinux.mailtunnel.eu/$repo/os/$arch": true,
            "https://mirrors.celianvdb.fr/archlinux/$repo/os/$arch": true,
            "https://mirror.oldsql.cc/archlinux/$repo/os/$arch": true,
            "https://mirrors.slaanesh.org/archlinux/$repo/os/$arch": true,
            "https://mirror.sysa.tech/archlinux/$repo/os/$arch": true,
            "https://mirror.thekinrar.fr/archlinux/$repo/os/$arch": true,
            "https://mirror.wormhole.eu/archlinux/$repo/os/$arch": true,
            "https://mirroir.wptheme.fr/archlinux/$repo/os/$arch": true,
            "https://arch.yourlabs.org/$repo/os/$arch": true
        }
    },
    "nic": {
        "NetworkManager": true
    },
    "ntp": "true",
    "packages": ["git", "ansible"],
    "profile": null,
    "superusers": {
        "mike": {
            "!password": "password"
        }
    },
    "sys-enconding": "utf-8",
    "sys-language": "en_US",
    "timezone": "Europe/Lisbon"
}